{
  "name": "llmeditor",
  "version": "0.1.0",
  "description": "llmeditor root package with workspaces",
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "scripts": {
    "next": "yarn workspace llmeditor-next",
    "ws": "yarn workspace llmeditor-websocket-server",
    "dev": "concurrently \"yarn next dev\" \"yarn ws start\""
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  },
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "node-gyp": "^9.4.0"
  }
}
